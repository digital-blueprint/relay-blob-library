{
  "extends": [
    "config:recommended",
    "group:allNonMajor",
    "schedule:weekends",
    ":automergePatch"
  ],
  "ignorePresets": [
    ":prHourlyLimit2"
  ],
  "rangeStrategy": "update-lockfile",
  "lockFileMaintenance": {
    "enabled": true
  },
  "constraintsFiltering": "strict",
  "packageRules": [
    {
      "allowedVersions": "<6",
      "matchPackageNames": [
        "symfony/{/,}**"
      ]
    },
    {
      "matchPackageNames": [
        "phpunit/phpunit"
      ],
      "allowedVersions": "<10"
    }
  ]
}
